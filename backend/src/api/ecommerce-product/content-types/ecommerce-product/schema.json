{
  "kind": "collectionType",
  "collectionName": "ecommerce_products",
  "info": {
    "singularName": "ecommerce-product",
    "pluralName": "ecommerce-products",
    "displayName": "Ecommerce Product",
    "name": "ecommerce-product",
    "description": "Product type for ecommerce"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "decimal",
      "default": 0
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ecommerce-category.ecommerce-category",
      "inversedBy": "products"
    },
    "metadata": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ecommerce-metadata.ecommerce-metadata",
      "inversedBy": "products"
    },
    "supplier": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ecommerce-supplier.ecommerce-supplier",
      "inversedBy": "products"
    }
  }
}
